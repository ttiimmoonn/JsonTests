  "PreConf": [
    {
      "command1": "/domain/%%DEV_DOM%%/sip/user/set %%0.SipGroup%% %%0.Number%%@%%0.SipDomain%% rfc-4028-control false",
      "command2": "/domain/%%DEV_DOM%%/sip/user/set %%1.SipGroup%% %%1.Number%%@%%1.SipDomain%% rfc-4028-control true",
      "command3": "/domain/%%DEV_DOM%%/timers/sip/set rfc4028_min_se 90",
      "command4": "/domain/%%DEV_DOM%%/timers/sip/set rfc4028_use_se 90"
    }
  ],
  "PostConf": [
    {
      "command1": "/domain/%%DEV_DOM%%/sip/user/set %%0.SipGroup%% %%0.Number%%@%%0.SipDomain%% rfc-4028-control true"
    }
  ],
  "Tests": [
    {
      "Name": "rfc4028_handling_422_response_from_uas",
      "Description": "Проверка корректности обработки 422 response. SSW отправляет INVITE c заголовком Session-Expires: 90, UAS отвечает 422 ответом с Min-SE: 100. Ожидается, что SSW отправит новый INVITE c Session-Expires: 100",
      "TestProcedure": [
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 1,
              "Name": "UAS_SEND_422_RESPONSE",
              "Commands": [
                {
                  "SourceFile": "rfc4028_timers/uas_send_422.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "200s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 0,
              "Name": "UAC_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN %%1.Number%% -set CDPNDOM %%1.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "200s"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "Name": "rfc4028_send_initial_invite_with_small_session_expires",
      "Description": "Посылаем INVITE c Session-Expires: 60. Ожидается, что SSW ответит 422 ответом",
      "TestProcedure": [
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 0,
              "Name": "UAS_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_recv_bye.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "200s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 1,
              "WriteStat": true,
              "Name": "UAC_RFC4028_RECV_422_TEST",
              "Commands": [
                {
                  "SourceFile": "rfc4028_timers/uac_recv_422.xml",
                  "Options": "-m 1 -set CDPN %%0.Number%% -set CDPNDOM %%0.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "200s"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "Name": "rfc4028_send_re_invite_with_small_session_expires",
      "Description": "Посылаем INVITE c Session-Expires: 60, получаем 422 от SSW. Далее присылаем re-invite c sendonly и снова ставим Session-Expires: 60. Ожидается, что SSW ответит 422 ответом",
      "TestProcedure": [
        {
          "SendSSHCommand": [
            {
              "command1": "/domain/%%DEV_DOM%%/ss/enable %%1.Number%% chold",
              "command2": "/domain/%%DEV_DOM%%/ss/activate %%1.Number%% chold"
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 0,
              "Name": "SIMPLE_UAS_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_recv_bye.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "200s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 1,
              "WriteStat": true,
              "Name": "UAC_RFC4028_RECV_422_TEST",
              "Commands": [
                {
                  "SourceFile": "rfc4028_timers/uac_send_small_se_in_re_invite.xml",
                  "Options": "-m 1 -set CDPN %%0.Number%% -set CDPNDOM %%0.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "200s"
                }
              ]
            }
          ]
        },
        {
          "SendSSHCommand": [
            {
              "command1": "/domain/%%DEV_DOM%%/ss/deactivate %%1.Number%% chold",
              "command2": "/domain/%%DEV_DOM%%/ss/disable %%1.Number%% chold"
            }
          ]
        }
      ]
    }
  ]
}