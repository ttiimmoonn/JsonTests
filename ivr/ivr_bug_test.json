      "Name": "send_ABCD_dtmf",
      "Description": "Падал IVR при отправке ABCD dtmf в блоке ivr",
      "TestProcedure": [
       {
          "StartUA": [
            {
              "Type": "User",
              "UserId" : 1,
              "WriteStat" : true,
              "Name": "UAC_SEND_ABCD_DTMF",
              "Commands": [
                {
                  "SourceFile": "ivr/uac_send_abcd_dtmf.xml",
                  "Options": "-m 1 -set CDPN A005 -set CDPNDOM %%1.SipDomain%% -aa",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "20s"
                }
              ]
            }
          ]
        }
