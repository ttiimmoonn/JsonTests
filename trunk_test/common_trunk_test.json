
{
  "TestName": "trunk_common_test",
  "AutoTest" : true,
  "Trunks" : [
    {
      "TrunkId" : 1,
      "TrunkName" : "sip_common_test1",
      "Port" : 5094,
      "SipDomain" : "%%DEV_DOM%%",
      "SipGroup" : "sip_transport"
    },
    {
      "TrunkId" : 2,
      "TrunkName" : "sip_common_test2",
      "Port" : 5095,
      "SipDomain" : "%%DEV_DOM%%",
      "SipGroup" : "sip_transport"
    }
   ],

"UserVar": [
    {
      "%%to_trunk2_ctx%%" : "",
      "%%trunk1_ctx_name%%" : "ctx_for_common_trunk_test",
      "%%ref_num%%" : "2001",
      "%%cdpn_num%%" : "2000",
      "%%cgpn_num%%" : "1000",
      "%%cdpn_with_hash%%" : "2000#",
      "%%cdpn_with_hash_code%%" : "2000%23",
      "%%cgpn_with_hash%%" : "1000#",
      "%%cgpn_with_hash_code%%" : "1000%23",
      "%%tr_cdpn%%" : "8913",
      "%%tr_cgpn%%" : "8912"
    }
],

  "PreConf": [
    {
      "command1" : "/domain/%%DEV_DOM%%/routing/.xbin/.import-context --xml-base64 %%to_trunk2_ctx%%",
      "command2" : "/domain/%%DEV_DOM%%/trunk/sip/declare %%trunk1_ctx_name%% %%Tr.1.SipGroup%% %%Tr.1.TrunkName%% %%SSW_IPSET%% static %%IP%% %%Tr.1.Port%% sip-proxy %%EXTER_PORT%%",
      "command3" : "/domain/%%DEV_DOM%%/trunk/sip/declare default.routing %%Tr.2.SipGroup%% %%Tr.2.TrunkName%% %%SSW_IPSET%% static %%IP%% %%Tr.2.Port%% sip-proxy %%EXTER_PORT%%"
    }
  ],
  "PostConf": [
    {
      "command1" : "/domain/%%DEV_DOM%%/trunk/sip/remove %%Tr.1.SipGroup%% %%Tr.1.TrunkName%% --force",
      "command2" : "/domain/%%DEV_DOM%%/trunk/sip/remove %%Tr.2.SipGroup%% %%Tr.2.TrunkName%% --force",
      "command3" : "/domain/%%DEV_DOM%%/routing/delete %%trunk1_ctx_name%%"
    }
  ],
  "Tests": [
    {
      "Name": "referred-by-as-cgpn-test",
      "Description": "Проверка корректности работы настройки referred-by-as-cgpn.",
      "TestProcedure": [
        {
          "SendSSHCommand": [
            {
              "command1": "/domain/%%DEV_DOM%%/trunk/sip/set %%Tr.1.SipGroup%% %%Tr.1.TrunkName%% referred-by-as-cgpn true"
            }
          ]
        },
        {
          "Sleep" : 2
        },
        {
          "StartUA": [
            {
              "Type": "Trunk",
              "TrunkId" : 2,
              "Name": "UAS_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_recv_bye.xml",
                  "Options": "-m 1 -set CHK_CGPN %%cgpn_num%%",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "Trunk",
              "TrunkId" : 1,
              "WriteStat": true,
              "Name": "UAC_INVITE_WOUT_REFERRED_BY_HDR",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_send_bye.xml",
                  "Options": "-m 1 -set CDPN %%cdpn_num%% -set CDPNDOM %%Tr.1.SipDomain%% -set CGPN %%cgpn_num%% -set CGPNDOM %%Tr.2.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "15s"
                }
              ]
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "Trunk",
              "TrunkId" : 2,
              "Name": "UAS_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_recv_bye.xml",
                  "Options": "-m 1 -set CHK_CGPN %%ref_num%%",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "Trunk",
              "TrunkId" : 1,
              "WriteStat": true,
              "Name": "UAC_INVITE_WITH_REFERRED_BY_HDR",
              "Commands": [
                {
                  "SourceFile": "trunk_test/uac_invite_with_referred_by_hdr.xml",
                  "Options": "-m 1 -set CDPN %%cdpn_num%% -set CDPNDOM %%Tr.1.SipDomain%% -set CGPN %%cgpn_num%% -set CGPNDOM %%Tr.2.SipDomain%% -set REF_NUM %%ref_num%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "15s"
                }
              ]
            }
          ]
        },
        {
          "SendSSHCommand": [
            {
              "command1": "/domain/%%DEV_DOM%%/trunk/sip/set %%Tr.1.SipGroup%% %%Tr.1.TrunkName%% referred-by-as-cgpn false"
            }
          ]
        }
      ]
    },
    {
      "Name": "symbol-hash-as-is-test",
      "Description": "Проверка корректности работы настройки symbol-hash-as-is.",
      "TestProcedure": [
        {
          "SendSSHCommand": [
            {
              "command1": "/domain/%%DEV_DOM%%/trunk/sip/set %%Tr.2.SipGroup%% %%Tr.2.TrunkName%% symbol_hash_as_is true"
            }
          ]
        },
        {
          "Sleep" : 2
        },
        {
          "StartUA": [
            {
              "Type": "Trunk",
              "TrunkId" : 2,
              "Name": "UAS_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_recv_bye.xml",
                  "Options": "-m 1 -set CHK_CDPN %%cdpn_with_hash%% -set CHK_CGPN %%cgpn_with_hash%%",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "Trunk",
              "TrunkId" : 1,
              "WriteStat": true,
              "Name": "UAC_CDPN_WITH_HASH",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_send_bye.xml",
                  "Options": "-m 1 -set CDPN %%cdpn_with_hash%% -set CDPNDOM %%Tr.1.SipDomain%% -set CGPN %%cgpn_with_hash%% -set CGPNDOM %%Tr.2.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "15s"
                }
              ]
            }
          ]
        },
        {
          "SendSSHCommand": [
            {
              "command1": "/domain/%%DEV_DOM%%/trunk/sip/set %%Tr.2.SipGroup%% %%Tr.2.TrunkName%% symbol_hash_as_is false"
            }
          ]
        },
        {
          "Sleep" : 2
        },
        {
          "StartUA": [
            {
              "Type": "Trunk",
              "TrunkId" : 2,
              "Name": "UAS_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_recv_bye.xml",
                  "Options": "-m 1 -set CHK_CDPN %%cdpn_with_hash_code%% -set CHK_CGPN %%cgpn_with_hash_code%%",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "Trunk",
              "TrunkId" : 1,
              "WriteStat": true,
              "Name": "UAC_CDPN_WITH_HASH",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_send_bye.xml",
                  "Options": "-m 1 -set CDPN %%cdpn_with_hash%% -set CDPNDOM %%Tr.1.SipDomain%% -set CGPN %%cgpn_with_hash%% -set CGPNDOM %%Tr.2.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "15s"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "Name": "send_t38_in_update",
      "Description": "Отправка t38 в UPDATE в предответке",
      "TestProcedure": [
        {
          "StartUA": [
            {
              "Type": "Trunk",
              "TrunkId" : 2,
              "Name": "UAS_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "trunk_test/uas_send_asdp_in_180.xml",
                  "Options": "-m 1 -d 1000",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "Trunk",
              "TrunkId" : 1,
              "WriteStat": true,
              "Name": "UAC_UPDATE",
              "Commands": [
                {
                  "SourceFile": "trunk_test/uac_update_to_t38.xml",
                  "Options": "-m 1 -set CDPN %%tr_cdpn%% -set CDPNDOM %%Tr.2.SipDomain%% -set CGPN %%tr_cgpn%% -set CGPNDOM %%Tr.1.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "15s"
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}