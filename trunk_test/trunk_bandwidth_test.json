{
  "TestName": "trunk_bandwidth_test",
  "AutoTest": true,
  "Trunks": [
    {
      "TrunkId": 1,
      "TrunkName": "sip_trunk_bandwidth1",
      "Port": 5031,
      "SipDomain": "%%DEV_DOM%%",
      "SipGroup": "sip_transport"
    },
    {
      "TrunkId": 2,
      "TrunkName": "sip_trunk_bandwidth2",
      "Port": 5032,
      "SipDomain": "%%DEV_DOM%%",
      "SipGroup": "sip_transport"
    }
  ],
  "UserVar": [
    {
      "%%to_trunk_ctx%%": "",
      "%%trunk_ctx_name%%": "ctx_trunk_bandwidth_test",
      "%%cdpn_num%%" : "900",
      "%%cgpn_num%%" : "800"
    }
  ],
  "PreConf": [
    {
      "command1": "/domain/%%DEV_DOM%%/routing/.xbin/.import-context --xml-base64 %%to_trunk_ctx%%",
      "command2": "/domain/%%DEV_DOM%%/trunk/sip/declare %%trunk_ctx_name%% %%Tr.1.SipGroup%% %%Tr.1.TrunkName%% %%SSW_IPSET%% static %%IP%% %%Tr.1.Port%% sip-proxy %%EXTER_PORT%%",
      "command3": "/domain/%%DEV_DOM%%/trunk/sip/declare default.routing %%Tr.2.SipGroup%% %%Tr.2.TrunkName%% %%SSW_IPSET%% static %%IP%% %%Tr.2.Port%% sip-proxy %%EXTER_PORT%%"
    }
  ],
  "PostConf": [
    {
      "command1": "/domain/%%DEV_DOM%%/trunk/sip/remove %%Tr.1.SipGroup%% %%Tr.1.TrunkName%% --force",
      "command2": "/domain/%%DEV_DOM%%/trunk/sip/remove %%Tr.2.SipGroup%% %%Tr.2.TrunkName%% --force",
      "command3": "/domain/%%DEV_DOM%%/routing/delete %%trunk_ctx_name%%"
    }
  ],
  "Tests": [
    {
      "Name": "test_bandwidth_in",
      "Description": "Проверка ограничения входящих вызовов на транке. Устанавливаем параметр bandwidth\\in в 2, делаем 3 входящих вызова на данный транк. Ожидается, что два вызова будут обработаны, а третий будет отбит 480 ответом. Далее производим очистку лимитов и делаем 3 вызова. Ожидается, что все вызовы будут обработаны.",
      "TestProcedure": [
        {
          "SendSSHCommand": [
            {
              "command1": "/domain/%%DEV_DOM%%/trunk/set %%Tr.1.SipGroup%% %%Tr.1.TrunkName%% bandwidth\\in 2",
              "command2": "/domain/%%DEV_DOM%%/trunk/set %%Tr.1.SipGroup%% %%Tr.1.TrunkName%% bandwidth\\out 0"
            }
          ]
        },
        {
          "Sleep": 2
        },
        {
          "StartUA": [
            {
              "Type": "Trunk",
              "TrunkId": 2,
              "Name": "UAS_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 2 -set CHK_CGPN %%cgpn_num%% -d 5000",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "Trunk",
              "TrunkId": 1,
              "WriteStat": true,
              "Name": "UAC_RECV_DROP_BY_COUNT",
              "Commands": [
                {
                  "SourceFile": "trunk_test/uac_recv_drop_call_by_count.xml",
                  "Options": "-m 3 -r 1 -l 3 -set CDPN %%cdpn_num%% -set CDPNDOM %%Tr.2.SipDomain%% -set CGPN %%cgpn_num%% -set CGPNDOM %%Tr.1.SipDomain%% -set DROP_COUNT 3",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "15s"
                }
              ]
            }
          ]
        },
        {
          "SendSSHCommand": [
            {
              "command1": "/domain/%%DEV_DOM%%/trunk/clean %%Tr.1.SipGroup%% %%Tr.1.TrunkName%% bandwidth\\in",
              "command2": "/domain/%%DEV_DOM%%/trunk/clean %%Tr.1.SipGroup%% %%Tr.1.TrunkName%% bandwidth\\out",
              "command3": "/domain/%%DEV_DOM%%/trunk/clean %%Tr.2.SipGroup%% %%Tr.2.TrunkName%% bandwidth\\in",
              "command4": "/domain/%%DEV_DOM%%/trunk/clean %%Tr.2.SipGroup%% %%Tr.2.TrunkName%% bandwidth\\out"
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "Trunk",
              "TrunkId": 2,
              "Name": "UAS_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 3 -set CHK_CGPN %%cgpn_num%% -d 5000",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "Trunk",
              "TrunkId": 1,
              "WriteStat": true,
              "Name": "UAC_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 3 -r 1 -l 3 -set CDPN %%cdpn_num%% -set CDPNDOM %%Tr.2.SipDomain%% -set CGPN %%cgpn_num%% -set CGPNDOM %%Tr.1.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "15s"
                }
              ]
            }
          ]
        }
      ]
    },
