"Tests": [
    {
      "Name": "test_outgoing_call_from_trunk_with_reg_with_100rel",
      "Description": "Проверка каскадной обработки 302 ответа. Вызов приходит с транка на SSW. Первый и второй абонент отвечают 302 сообщением по причине занятости пользователя. Ожидается, что SSW последовательно перешлет вызов и диалог успешно завершится с 3 абонентом.",
      "TestProcedure": [
        
        {
          "SendSSHCommand": [
            "/domain/%%DEV_DOM%%/trunk/sip/info %%Tr.1.SipGroup%% %%Tr.1.TrunkName%%"
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 0,
              "WriteStat": true,
              "Name": "UAS_TEST_SEND_DIVERSION",
              "Commands": [
                {
                  "SourceFile": "trunk_test/uas_send_302_one_header_diversion.xml",
                  "Options": "-m 1 -set CDPN_NEW %%1.Number%%",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "50s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 1,
              "WriteStat": true,
              "Name": "UAS_TEST_SEND_DIVERSION_TWO",
              "Commands": [
                {
                  "SourceFile": "trunk_test/uas_send_302_two_header_diversion.xml",
                  "Options": "-m 1 -set SERV_IP %%EXTER_IP%% -set SERV_PORT %%EXTER_PORT%% -set CDPN_ORIG %%0.Number%% -set CDPN_OLD %%1.Number%% -set CDPN_NEW %%2.Number%%",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "60s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 2,
              "WriteStat": true,
              "Name": "UAS_TEST",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -d 1000",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "70s"
                }
              ]
            },
            {
              "Type": "Trunk",
              "TrunkId": 1,
              "WriteStat": true,
              "Name": "UAC_TEST",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN %%0.Number%% -set CGPN %%trunk_number%% -set CDPNDOM %%0.SipDomain%% -set CGPNDOM %%Tr.1.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "60s"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "Name": "test_outgoing_call_from_trunk_with_reg_with_100rel",
      "Description": "Проверка diversion в INV. Вызов, в котором уже присутствует сообщение diversion, поступает с транка на абонента SSW. Абонент отвечает 302, после чего SSW отправляет INV на следующего абонента. Ожидается, что после переадресации диалог завершится успешно.",
      "TestProcedure": [
        
