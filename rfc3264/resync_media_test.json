              "Name": "UAC_RECV_RE_INVITE",
              "Commands": [
                {
                  "SourceFile": "rfc3264/uac_recv_re_invite.xml",
                  "Options": "-m 1 -set CDPN %%0.Number%% -set CDPNDOM %%0.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
