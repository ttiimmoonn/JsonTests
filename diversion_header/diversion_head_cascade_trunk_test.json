"Tests": [
    {
      "Name": "test_trunk_recv_diversion_header",
      "Description": "Проверка каскадной обработки 302 ответа. У Транка_2 и Абонента_0 активируется услуга cf_sip. Делаем вызов с Транка_1 на Транк_2.  Транк_2 отвечает 302 сообщением, указав: в Contact URI Абонента_0, а в поле Diversion URI Транка 2. Ожидается, что SSW корректно обработает сообщение и отправит INVITE c RURI из Contact 302 сообщения, сохранив заголовки From, To, Diversion. Абонент_0 отвечает 302 сообщением, указав в Contact URI абонента из Транка_3, добавив второй заголовок Diversion c URI Абонента_0. Ожидается, что вызов с Транком_3 завершиться успешно, а в INVITE запросе будет указан заголовок From Транка_1 и заголовок To Транка_2.",
      "TestProcedure": [
        {
          "StartUA": [
            {
              "Type": "Trunk",
              "TrunkId": 2,
              "WriteStat": true,
              "Name": "UAS_TEST_SEND_DIVERSION",
              "Commands": [
                {
                  "SourceFile": "diversion_header/uas_send_302_one_header_diversion.xml",
                  "Options": "-m 1 -set CDPN_NEW %%0.Number%%",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "50s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 0,
              "WriteStat": true,
              "Name": "UAS_TEST_SEND_DIVERSION_TWO",
              "Commands": [
                {
                  "SourceFile": "diversion_header/uas_send_302_two_header_diversion.xml",
                  "Options": "-m 1 -set SERV_IP %%EXTER_IP%% -set SERV_PORT %%EXTER_PORT%% -set CDPN_ORIG %%test_trunk_2_diversion_recv_302_number%% -set CDPN_NEW %%test_trunk_3_diversion_send_bye_number%%",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "60s"
                }
              ]
            },
            {
              "Type": "Trunk",
              "TrunkId": 3,
              "WriteStat": true,
              "Name": "UAS_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -d 1000 -set CHK_CGPN %%test_trunk_1_diversion_send_invite_number%% -set CHK_CDPN %%test_trunk_2_diversion_recv_302_number%%",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "70s"
                }
              ]
            },
            {
              "Type": "Trunk",
              "TrunkId": 1,
              "WriteStat": true,
              "Name": "UAC_TEST",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN %%test_trunk_2_diversion_recv_302_number%% -set CGPN %%test_trunk_1_diversion_send_invite_number%% -set CDPNDOM %%Tr.2.SipDomain%% -set CGPNDOM %%Tr.1.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "60s"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "Name": "test_trunk_send_diversion_header",
      "Description": "Проверка каскадной обработки 302 ответа при INVITE из транка с заголовком Diversion. У Транка_2 активируем услугу cf_sip. Делаем вызов с Транка_1 на Транк_2 по средствам INVITE с заголовком Diversion, где указан номер cdpn_orig, который якобы отказался от диалога 302 сообщением, так же cdpn_orig указан в To, а в RURI указан номер абонента из транка. Ожидается, что SSW корректно обработает INVITE, отправит его в Транк_2, оставив заголовки From, To и Diversion неизменными. Транк_2 отвечает 302 сообщением, указав в Contact URI абонента из Транка_3, добавив второй заголовок Diversion c URI абонента из Транка_2. Ожидается, что вызов с Транком_3 завершиться успешно, а в INVITE запросе будет указан заголовок From Транка_1 и заголовок To cdpn_orig.",
      "TestProcedure": [
        {
          "SendSSHCommand": [
            "/domain/%%DEV_DOM%%/iface/user-set sip1 %%Tr.2.SipGroup%% %%Tr.2.TrunkName%% routing.context %%ctx_to_trunk_3%%"
          ]
        },
        {
          "StartUA": [
            {
              "Type": "Trunk",
              "TrunkId": 2,
              "WriteStat": true,
              "Name": "UAS_TEST_SEND_DIVERSION_TWO",
              "Commands": [
                {
                  "SourceFile": "diversion_header/uas_send_302_two_header_diversion.xml",
                  "Options": "-m 1 -set SERV_IP %%IP%% -set CDPN_ORIG %%0.Number%% -set CDPN_NEW %%test_trunk_3_diversion_send_bye_number%%",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "60s"
                }
              ]
            },
            {
              "Type": "Trunk",
              "TrunkId": 3,
              "WriteStat": true,
              "Name": "UAS_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -d 1000 -set CHK_CGPN %%test_trunk_1_diversion_send_invite_number%% -set CHK_CDPN %%0.Number%%",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "70s"
                }
              ]
            },
            {
              "Type": "Trunk",
              "TrunkId": 1,
              "WriteStat": true,
              "Name": "UAC_TEST_SEND_DIVER_HEADER",
              "Commands": [
                {
                  "SourceFile": "diversion_header/uac_send_divers_header.xml",
                  "Options": "-m 1 -set CDPN_ORIG %%0.Number%% -set CDPN %%test_trunk_2_diversion_recv_302_number%% -set CGPN %%test_trunk_1_diversion_send_invite_number%% -set CDPNDOM %%Tr.2.SipDomain%% -set CGPNDOM %%Tr.1.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "60s"
                }
              ]
            }
          ]
        },
        {
          "SendSSHCommand": [
            "/domain/%%DEV_DOM%%/iface/user-set sip1 %%Tr.2.SipGroup%% %%Tr.2.TrunkName%% routing.context %%SSW_DEF_ROUTE%%"
          ]
        }
      ]
