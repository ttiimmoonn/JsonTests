  "Tests": [
    {
      "Name": "forking all-contact test",
      "Description": "Звоним на номер с форкингом и отвечаем с одного из контактов. Вызов проключается, всем остальным высылается CANCEL. UA используют tcp",
      "TestProcedure": [
        {
          "SendSSHCommand": [
            "/domain/%%DEV_DOM%%/sip/user/set %%0.SipGroup%% %%0.Number%%@%%0.SipDomain%% fork-mode all-contacts"
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 0,
              "WriteStat": true,
              "Name": "UAS0_FORK_TCP",
              "Commands": [
                {
                  "SourceFile": "forking/uas_recv_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "20s"
                },
                {
                  "SourceFile": "forking/uas_recv_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "20s"
                },
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -d 5000",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 1,
              "WriteStat": true,
              "Name": "UAS1_FORK_TCP",
              "Commands": [
                {
                  "SourceFile": "forking/uas_recv_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "20s"
                },
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -d 5000",
                  "SippType": "uas",
                  "Timeout": "20s"
                },
                {
                  "SourceFile": "forking/uas_recv_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 2,
              "WriteStat": true,
              "Name": "UAS2_FORK_TCP",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -d 5000",
                  "SippType": "uas",
                  "Timeout": "20s"
                },
                {
                  "SourceFile": "forking/uas_recv_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "20s"
                },
                {
                  "SourceFile": "forking/uas_recv_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 3,
              "Name": "UAC_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 3 -r 1 -l 1 -d 3000 -set CDPN %%0.Number%% -set CDPNDOM %%0.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "60s"
                }
              ]
            }
          ]
        },
        {
          "CheckDifference": [
            {
              "Difference": 0,
              "CompareMode": "between_ua",
              "SearchMode": "between_calls",
              "UA": "user:0,user:1,user:2",
              "Msg": [
                {
                  "MsgType": "request",
                  "Method": "INVITE",
                  "Code": null
                }
              ]
            },
            {
              "Difference": 0,
              "CompareMode": "between_ua",
              "SearchMode": "between_calls",
              "Calls": [
                1
              ],
              "UA": "user:0,user:1",
              "Msg": [
                {
                  "MsgType": "request",
                  "Method": "CANCEL",
                  "Code": null
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "Name": "forking one contact not reachable test",
      "Description": "Звоним на номер с форкингом. При этом на одном из контактов не поднимаем UA, чтобы позникла ошибка по транспорту.",
      "TestProcedure": [
        {
          "SendSSHCommand": [
            "/domain/%%DEV_DOM%%/sip/user/set %%0.SipGroup%% %%0.Number%%@%%0.SipDomain%% fork-mode all-contacts"
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 1,
              "WriteStat": true,
              "Name": "UAS1_RECV_CANEL",
              "Commands": [
                {
                  "SourceFile": "forking/uas_recv_cancel.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 2,
              "WriteStat": true,
              "Name": "UAS2_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -d 2000",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 3,
              "Name": "UAC_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN %%0.Number%% -set CDPNDOM %%0.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "20s"
                }
              ]
            }
          ]
        },
        {
          "CheckDifference": [
            {
              "Difference": 0,
              "CompareMode": "between_ua",
              "SearchMode": "between_calls",
              "UA": "user:1,user:2",
              "Msg": [
                {
                  "MsgType": "request",
                  "Method": "INVITE",
                  "Code": null
                }
              ]
            }
          ]
        }
      ]
    },
    {
