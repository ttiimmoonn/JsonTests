              "Name": "UAS_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -d 2000 -set CHK_DNAME %%REMOTE_DNAME%% -set CHK_CGPN %%0.Number%%",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "20s"
                }
