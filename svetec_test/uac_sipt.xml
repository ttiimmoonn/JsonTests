<scenario name="Basic Sipstone UAC">


<Global variables="CDPNDOM,CGPNDOM,CDPN,CGPN" />
    <nop hide="true">
    <action>
        <assignstr assign_to="cgpn_dom" value="[$CGPNDOM]"/>
        <assignstr assign_to="cdpn_dom" value="[$CDPNDOM]"/>
        <assignstr assign_to="cdpn" value="[$CDPN]"/>
        <assignstr assign_to="cgpn" value="[$CGPN]"/>
    </action>
    </nop>

<send retrans="500">
<![CDATA[
INVITE sip:[$cdpn]@[remote_ip]:[remote_port] SIP/2.0
Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
From: "SimpleTest" <sip:[$cgpn]@[$cgpn_dom]>;tag=[call_number]
To: "[$cdpn]" <sip:[$cdpn]@[$cdpn_dom]>
Call-ID: [call_id]
CSeq: [cseq] INVITE
Contact: <sip:[$cgpn]@[local_ip]:[local_port]>
Max-Forwards: 70
Content-Type: multipart/mixed;boundary=unique-boundary-1
Content-Length: [len]

--unique-boundary-1
Content-Type: application/sdp

v=0
o=user1 53655765 2353687637 IN IP[local_ip_type] [local_ip]
s=-
c=IN IP[media_ip_type] [media_ip]
t=0 0
m=audio [media_port] RTP/AVP 0
a=rtpmap:0 PCMU/8000

--unique-boundary-1
Content-Type: application/ISUP;version=itu-t92+
Content-Disposition: signal; handling=optional

\x01\x00\x60\x00\x0a\x03\x02\x07\x05\x81\x10\x13\x80\x0f\x0a\x07\x03\x13\x19\x33\x39\x76\x17\x3f\x04\x83\x13\x44\x05\x00
--unique-boundary-1--

]]>
</send>


<recv response="100" optional="true"/>
<recv response="180" optional="true"/>
<recv response="181" optional="true"/>
<recv response="183" optional="true"/>
<!-- <recv response="302" rtd="true"/> -->
<recv response="200" rtd="true"> 
        <action>
         <!--Забрали CSeq -->
        <ereg regexp="[0-9]+" search_in="hdr" header="CSeq:" assign_to="last_cseq" />
        </action>
</recv> 

<send crlf="true">
<![CDATA[
    ACK sip:[$cdpn]@[remote_ip]:[remote_port] SIP/2.0
    Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
    [last_from:]
    [last_to:]
    [last_call-id:]
    CSeq: [$last_cseq] ACK
    Content-Length: 0
]]>
</send>

<pause milliseconds = "1000"/>

<send retrans="500">
<![CDATA[
BYE sip:[$cdpn]@[remote_ip]:[remote_port] SIP/2.0
Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
From: "SimpleTest" <sip:[$cgpn]@[$cgpn_dom]>;tag=[call_number]
To: "[$cdpn]" <sip:[$cdpn]@[$cdpn_dom]>[peer_tag_param]
Call-ID: [call_id]
CSeq: 2 BYE
Contact: <sip:[$cgpn]@[local_ip]:[local_port]>
Max-Forwards: 70
Reason: Q.850;cause=16;text="Normal call clearing"
Content-Type: application/ISUP;version=itu-t92+
Content-Length: [len]

\x0c\x02\x00\x02\x80\x9b

]]>
</send>


<recv response="200" crlf="true"/>

<!-- definition of the response time repartition table (unit is ms)   -->
<ResponseTimeRepartition value="200, 300, 400,500,600,700,800,900"/>

<!-- definition of the call length repartition table (unit is ms)     -->
<CallLengthRepartition value="2300, 2400,2500,2600,2700,2800,2900,3000,3100,3200,3300,3400"/>

</scenario>
