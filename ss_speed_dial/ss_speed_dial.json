"Tests": [
    {
      "Name": "simple_last_number_redial_test",
      "Description": "Активируем услугу speed_dial. Делаем вызовы на короткие номера. Деактивируем услугу и проверяем, что она деактивировалась",
      "TestProcedure": [
        {
          "ServiceFeature": [
            {
              "code": "*51*1*%%1.Number%%#",
              "userId" : 0
            }
          ]
        },
        {
          "ServiceFeature": [
            {
              "code": "*51*2*%%2.Number%%#",
              "userId" : 0
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId" : 1,
              "Name": "UAS1_SIMPLE_REDIAL_TEST",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId" : 2,
              "Name": "UAS2_SIMPLE_REDIAL_TEST",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "40s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId" : 0,
              "Name": "UAC_SIMPLE_REDIAL_TEST",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN **1 -set CDPNDOM %%0.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "10s"
                },
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN **2 -set CDPNDOM %%0.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "10s"
                }
              ]
            }
          ]
        },
        {
          "ServiceFeature": [
            {
              "code": "#51*1#",
              "userId" : 0
            }
          ]
        },
        {
          "ServiceFeature": [
            {
              "code": "#51*2#",
              "userId" : 0
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId" : 1,
              "Name": "UAC_RECV_404",
              "Commands": [
                {
                  "SourceFile": "ss_speed_dial/uac_recv_404.xml",
                  "Options": "-m 1 -set CDPN **1 -set CDPNDOM %%0.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "10s"
                },
                {
                  "SourceFile": "ss_speed_dial/uac_recv_404.xml",
