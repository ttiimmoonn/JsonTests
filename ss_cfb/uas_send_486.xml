<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE scenario SYSTEM "sipp.dtd">
<scenario name = "omg_wtf">

<recv request="INVITE"></recv>

<send>
<![CDATA[
    SIP/2.0 100 Trying
    [last_Via:]
    [last_From:]
    [last_To:]
    [last_Call-ID:]
    [last_CSeq:]
    Content-Length: 0
]]>
</send>


<pause milliseconds="3000"/>
<send>
<![CDATA[
    SIP/2.0 486 Busy Here
    [last_Via:]
    [last_From:]
    [last_To:];tag=[call_number][local_port]
    [last_Call-ID:]
    [last_CSeq:]
    [last_contact]
    Content-Length: 0
]]>
</send>

<recv request="ACK"></recv>
</scenario>
