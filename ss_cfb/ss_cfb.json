    {
      "Name": "simple_cfb_test",
      "Description": "Звоним на абонента с cfb, даём busy и проверяем переадресацию",
      "TestProcedure": [
        {
          "ServiceFeature": [
            {
              "code": "*22*%%0.Number%%#",
              "userId": 2
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 0,
              "Name": "SIMPLE_UAS_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -d 2000",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 2,
              "Name": "UAS_SEND_486",
              "Commands": [
                {
                  "SourceFile": "ss_cfb/uas_send_486.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 1,
              "Name": "UAC_RECV_181",
              "Commands": [
                {
                  "SourceFile": "ss_cfb/uac_recv_181.xml",
