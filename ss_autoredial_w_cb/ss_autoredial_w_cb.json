  "Tests": [
    {
      "Name": "simple_autoredial_w_cb_test",
      "Description": "Заказываем autoredial_w_cb. На стороне Б даём 2 раза busy, на третий раз отвечаем и ждём cb со стороны А",
      "TestProcedure": [
        {
          "SendSSHCommand": [
            "/domain/%%DEV_DOM%%/ss/activate %%0.Number%% auto_redial_with_callback a_no_answer_timeout = 5, b_no_answer_timeout = 5, recall_count = 3, recall_timer = 2"
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId": 1,
              "WriteStat": true,
              "Name": "UAS_FOR_TEST_AUTOREDIAL",
              "Commands": [
                {
                  "SourceFile": "ss_autoredial_w_cb/uas_send_486.xml",
                  "Options": "-m 2",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "10s"
                },
                {
                  "SourceFile": "simple_ua/uas_recv_bye.xml",
                  "Options": "-m 1",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "10s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId": 0,
              "WriteStat": true,
              "Name": "UAC_FOR_TEST_AUTOREDIAL",
              "Commands": [
                {
                  "SourceFile": "ss_autoredial_w_cb/uac_recv_487.xml",
                  "Options": "-m 1 -set CDPN *39*%%1.Number%%# -set CDPNDOM %%1.SipDomain%%",
                  "SippType": "uac",
                  "NeedAuth": true,
                  "Timeout": "10s"
                },
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -d 2000",
                  "SippType": "uas",
                  "NeedAuth": true,
                  "Timeout": "20s"
                }
              ]
            }
          ]
        }
      ]
    },
