              "Type": "User",
              "UserId" : 0,
              "Name": "UAC_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_send_bye.xml",
                  "Options": "-m 1 -set CDPN %%2.Number%% -set CDPNDOM %%2.SipDomain%% -d 200",
                  "SippType": "uac",
