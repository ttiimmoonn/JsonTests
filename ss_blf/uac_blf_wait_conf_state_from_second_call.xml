<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE scenario SYSTEM "sipp.dtd">
<scenario name = "blf_uac">

<Global variables="CDPNDOM,CGPNDOM,CDPN" />
<User variables="cdpn_dom,cgpn_dom,cdpn,check_state,dummy" />
<Reference variables="dummy" />
    <nop hide="true">
    <action>
        <assignstr assign_to="local_tag" value="[call_number][pid]"/>
        <assignstr assign_to="cgpn_dom" value="[$CGPNDOM]"/>
        <assignstr assign_to="cdpn_dom" value="[$CDPNDOM]"/>
        <assignstr assign_to="cdpn" value="[$CDPN]"/>
    </action>
    </nop>


<send>
<![CDATA[
    SUBSCRIBE sip:[$cdpn]@[remote_ip]:[remote_port] SIP/2.0
    Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
    From: "BLF тест" <sip:[service]@[$cgpn_dom]>;tag=[$local_tag]
    To: <sip:[$cdpn]@[$cdpn_dom]>
    Call-ID: [call_id]
    CSeq: [cseq] SUBSCRIBE
    Contact: <sip:[service]@[local_ip]:[local_port]>
    Event: dialog
    Accept: application/dialog-info+xml
    Expires: 90
    Content-Length: 0
]]>
</send>


<recv response="401" auth="true" optional="true" next="send_auth_to_subscribe"/>

<label id="recv_200_to_subscribe"/>


<recv response="200">
    <action>
        <ereg regexp=";tag=(\w+)" search_in="hdr" header="To:" check_it="true" assign_to="dummy,subscribe_remote_tag" />
    </action>
</recv>

<pause milliseconds="1000"/>

<recv request="NOTIFY">
    <action>
        <assign assign_to="dummy" value="" />
        <assign assign_to="check_state" value="" />
        <ereg regexp=".*(<state>trying</state>).*" search_in="msg" check_it="true" assign_to="dummy,check_state"/>
        <ereg regexp="[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}" search_in="hdr" header="Via:" assign_to="new_ip" />
    </action>
</recv>

<nop hide="true">
    <action>
        <setdest host="[$new_ip]" port="[remote_port]" protocol="UDP" />
    </action>
</nop>

<send>
<![CDATA[
    SIP/2.0 200 OK
    [last_Via:]
    [last_From:]
    [last_To:]
    [last_Call-ID:]
    [last_CSeq:]
    Content-Length: 0
]]>
</send>

<recv request="NOTIFY">
    <action>
        <assign assign_to="dummy" value="" />
        <assign assign_to="check_state" value="" />
        <ereg regexp=".*(<state>early</state>).*" search_in="msg" check_it="true" assign_to="dummy,check_state"/>
        <ereg regexp="[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}" search_in="hdr" header="Via:" assign_to="new_ip" />
    </action>
</recv>

<nop hide="true">
    <action>
        <setdest host="[$new_ip]" port="[remote_port]" protocol="UDP" />
    </action>
</nop>

<send>
<![CDATA[
    SIP/2.0 200 OK
    [last_Via:]
    [last_From:]
    [last_To:]
    [last_Call-ID:]
    [last_CSeq:]
    Content-Length: 0
]]>
</send>

<recv request="NOTIFY">
    <action>
        <assign assign_to="dummy" value="" />
        <assign assign_to="check_state" value="" />
        <ereg regexp=".*(<state>confirmed</state>).*" search_in="msg" check_it="true" assign_to="dummy,check_state"/>
        <ereg regexp="[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}" search_in="hdr" header="Via:" assign_to="new_ip" />
    </action>
</recv>

<nop hide="true">
    <action>
        <setdest host="[$new_ip]" port="[remote_port]" protocol="UDP" />
    </action>
</nop>

<send>
<![CDATA[
    SIP/2.0 200 OK
    [last_Via:]
    [last_From:]
    [last_To:]
    [last_Call-ID:]
    [last_CSeq:]
    Content-Length: 0
]]>
</send>

<recv request="NOTIFY">
    <action>
        <assign assign_to="dummy" value="" />
        <assign assign_to="check_state" value="" />
        <ereg regexp=".*(<state>terminated</state>).*" search_in="msg" check_it="true" assign_to="dummy,check_state"/>
        <ereg regexp="[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}" search_in="hdr" header="Via:" assign_to="new_ip" />
        <ereg regexp="[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}" search_in="hdr" header="Contact:" assign_to="contact_ip" />
    </action>
</recv>

<nop hide="true">
    <action>
        <setdest host="[$new_ip]" port="[remote_port]" protocol="UDP" />
    </action>
</nop>

<send>
<![CDATA[
    SIP/2.0 200 OK
    [last_Via:]
    [last_From:]
    [last_To:]
    [last_Call-ID:]
    [last_CSeq:]
    Content-Length: 0
]]>
</send>

<recv request="NOTIFY">
    <action>
        <assign assign_to="dummy" value="" />
        <assign assign_to="check_state" value="" />
        <ereg regexp=".*(<state>confirmed</state>).*" search_in="msg" check_it="true" assign_to="dummy,check_state"/>
        <ereg regexp="[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}" search_in="hdr" header="Via:" assign_to="new_ip" />
    </action>
</recv>

<nop hide="true">
    <action>
        <setdest host="[$new_ip]" port="[remote_port]" protocol="UDP" />
    </action>
</nop>

<send>
<![CDATA[
    SIP/2.0 200 OK
    [last_Via:]
    [last_From:]
    [last_To:]
    [last_Call-ID:]
    [last_CSeq:]
    Content-Length: 0
]]>
</send>

<recv request="NOTIFY">
    <action>
        <assign assign_to="dummy" value="" />
        <assign assign_to="check_state" value="" />
        <ereg regexp=".*(<state>terminated</state>).*" search_in="msg" check_it="true" assign_to="dummy,check_state"/>
        <ereg regexp="[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}" search_in="hdr" header="Via:" assign_to="new_ip" />
        <ereg regexp="[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}" search_in="hdr" header="Contact:" assign_to="contact_ip" />
    </action>
</recv>

<nop hide="true">
    <action>
        <setdest host="[$new_ip]" port="[remote_port]" protocol="UDP" />
    </action>
</nop>

<send>
<![CDATA[
    SIP/2.0 200 OK
    [last_Via:]
    [last_From:]
    [last_To:]
    [last_Call-ID:]
    [last_CSeq:]
    Content-Length: 0
]]>
</send>


<pause milliseconds="2000"/>

<nop hide="true">
    <action>
        <setdest host="[$contact_ip]" port="[remote_port]" protocol="UDP" />
    </action>
</nop>


<send>
<![CDATA[
    SUBSCRIBE sip:[$cdpn]@[remote_ip]:[remote_port] SIP/2.0
    Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
    From: "BLF тест" <sip:[service]@[$cgpn_dom]>;tag=[$local_tag]
    To: <sip:[$cdpn]@[$cdpn_dom]>;tag=[$subscribe_remote_tag]
    [last_call-id:]
    CSeq: [cseq] SUBSCRIBE
    Contact: <sip:[service]@[local_ip]:[local_port]>
    Event: dialog
    Accept: application/dialog-info+xml
    Expires: 0
    Content-Length: 0
]]>
</send>

<recv response="401" auth="true" optional="true" next="send_auth_to_unsubscribe"/>

<label id="recv_200_to_unsubscribe"/>

<recv response="200" next="exit"></recv>


<label id="send_auth_to_subscribe"/>

<send next="recv_200_to_subscribe">
<![CDATA[
    SUBSCRIBE sip:[$cdpn]@[remote_ip]:[remote_port] SIP/2.0
    Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
    From: "BLF тест" <sip:[service]@[$cgpn_dom]>;tag=[$local_tag]
    To: <sip:[$cdpn]@[$cdpn_dom]>
    Call-ID: [call_id]
    [authentication]
    CSeq: [cseq] SUBSCRIBE
    Contact: <sip:[service]@[local_ip]:[local_port]>
    Event: dialog
    Accept: application/dialog-info+xml
    Expires: 60
    Content-Length: 0
]]>
</send>


<label id="send_auth_to_unsubscribe"/>

<send next="recv_200_to_unsubscribe">
<![CDATA[
    SUBSCRIBE sip:[$cdpn]@[remote_ip]:[remote_port] SIP/2.0
    Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
    From: "BLF тест" <sip:[service]@[$cgpn_dom]>;tag=[$local_tag]
    To: <sip:[$cdpn]@[$cdpn_dom]>;tag=[$subscribe_remote_tag]
    Call-ID: [call_id]
    [authentication]
    CSeq: [cseq] SUBSCRIBE
    Contact: <sip:[service]@[local_ip]:[local_port]>
    Event: dialog
    Accept: application/dialog-info+xml
    Expires: 0
    Content-Length: 0
]]>
</send>

<label id="exit"/>
<pause milliseconds="2000"/>
</scenario>
