  "Tests": [
    {
      "Name": "blf_forking_test",
      "Description": "Тестирование нотификаций при вызове на абонента, который использует forking",
      "TestProcedure": [
        {
          "SendSSHCommand": [
            {
              "command1": "/domain/%%DEV_DOM%%/sip/user/set %%0.SipGroup%% %%0.Number%%@%%0.SipDomain%% fork-mode all-contacts"
