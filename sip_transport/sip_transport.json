    {
      "Name": "test_tcp_only_mode",
      "Description": "Проверка режима tcp_only",
      "TestProcedure": [
        {
          "SendSSHCommand": [
            {
              "command1": "/domain/%%DEV_DOM%%/sip/properties/set sip_transport tcp_only"
            }
          ]
        },
        {
          "Sleep" : 2
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId" : 0,
              "Name": "TCP_UAS_SEND_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -aa",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId" : 1,
              "WriteStat" : true,
              "Name": "TCP_UAC_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN %%0.Number%% -set CDPNDOM %%0.SipDomain%% -aa",
                  "SippType": "uac",
                  "NeedAuth" : true,
                  "Timeout": "15s"
                },
                {
                  "SourceFile": "sip_transport/uac_recv_502.xml",
                  "Options": "-m 1 -set CDPN %%2.Number%% -set CDPNDOM %%2.SipDomain%% -aa",
                  "SippType": "uac",
                  "NeedAuth" : true,
                  "Timeout": "15s"
                }
              ]
            }
          ]
        },
        {
          "StartUA": [
            {
              "Type": "User",
              "UserId" : 4,
              "Name": "USER_WOUT_REQ_TRANSPORT",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uas_send_bye.xml",
                  "Options": "-m 1 -aa -t tn -max_socket 25",
                  "SippType": "uas",
                  "Timeout": "20s"
                }
              ]
            },
            {
              "Type": "User",
              "UserId" : 1,
              "WriteStat": true,
              "Name": "TCP_UAC_RECV_BYE",
              "Commands": [
                {
                  "SourceFile": "simple_ua/uac_recv_bye.xml",
                  "Options": "-m 1 -set CDPN %%4.Number%% -set CDPNDOM %%4.SipDomain%% -aa",
                  "SippType": "uac",
